namespace fbs;

struct LocationI {
    x: int;
    y: int;
    z: int;
}

struct LocationD {
    x: double;
    y: double;
    z: double;
}

table Chunk {
    location: LocationI;
}

table RegionUpdate {
    chunks: [Chunk];
}

union UpdateKind {
    Region: RegionUpdate
}

table Update {
    kind: UpdateKind;
}

root_type Update;
